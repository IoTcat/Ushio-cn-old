	function getMatchedKeys($str, $arr){
	    if(!is_array($str)){
	        $o = array();
	        foreach($arr as $key=>$val){
	            if($val >= $str - $GLOBALS['range'] && $val <= $str + $GLOBALS['range']) array_push($o, $key);
